{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "pretty": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "suppressImplicitAnyIndexErrors": false,
    "forceConsistentCasingInFileNames": true
  }
}
